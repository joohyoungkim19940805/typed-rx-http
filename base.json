{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "dom",
            "dom.Iterable",
            "ES2015",
            "ES2016",
            "ES2017",
            "ES2018",
            "ES2019",
            "ES2020",
            "ES2021",
            "ES2022",
            "ES2023",
            "esnext"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowJs": true
    }
}
